{
  "name": "demini-test-fixture-mixed",
  "version": "1.0.0",
  "private": true,
  "description": "Mixed CJS+ESM esbuild test bundle with known sourcemap for validation",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.js --bundle --minify --outfile=dist/bundle.js --platform=node --format=esm --metafile=dist/meta.json --sourcemap=external --external:typescript --banner:js=\"import{createRequire}from'module';const require=createRequire(import.meta.url);\"",
    "start": "node src/index.js",
    "start:bundle": "node dist/bundle.js"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "cosmiconfig": "^9.0.0",
    "debug": "^4.3.7",
    "dotenv": "^16.4.5",
    "execa": "^9.5.2",
    "find-up": "^7.0.0",
    "fs-extra": "^11.2.0",
    "globby": "^14.0.0",
    "got": "^14.0.0",
    "jose": "^6.0.8",
    "lodash": "^4.17.21",
    "meow": "^13.0.0",
    "ora": "^8.0.0",
    "p-queue": "^8.0.0",
    "semver": "^7.6.3",
    "uuid": "^9.0.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.3"
  }
}
